<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:background="@color/dark_blue">

    <View
        android:id="@+id/ellipse_start_page"
        android:layout_width="132dp"
        android:layout_height="132dp"
        android:background="@drawable/ic_ellipse_orange"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        android:layout_marginStart="135dp"/>
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/mark_pro"
        style="@style/font_800_white"
        android:textSize="30sp"
        android:text="Ecommerce\nConcept"
        app:layout_constraintTop_toTopOf="@id/ellipse_start_page"
        app:layout_constraintStart_toStartOf="@id/ellipse_start_page"
        android:layout_marginStart="34dp"
        android:layout_marginTop="32dp"/>


</androidx.constraintlayout.widget.ConstraintLayout>